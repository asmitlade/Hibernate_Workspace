<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        <hibernate-mapping>
        	<!-- class cfgs -->
        	<class name="com.mac.entity.BrandFactory" table="BRANDFACTORY">
        		<id name="custMobileNo" column="CUSTMOBILENO"/>
        		<property name="custName" column="CUSTNAME"/>
        		<property name="custAddrs" column="CUSTADDRS"/>
        		<property name="rewardPoints" column="REWARDPOINTS"/>
        	</class>
        </hibernate-mapping>